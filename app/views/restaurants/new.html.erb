<h1>Add a restaurant</h1>
<%= simple_form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label "Phone Number" %>
  <%= f.text_field :phone_number %>
  <%= f.submit %>
<% end %>
